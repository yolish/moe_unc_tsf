    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "moe",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "args": ["--task_name","long_term_forecast","--is_training","1","--model_id","test",
             "--model", "PatchTST", "--data", "ETTh1","--num_experts","3"]
        },
        {
            "name": "moe-prob",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "args": ["--task_name","long_term_forecast","--is_training","1","--model_id","test",
             "--model", "PatchTST", "--data", "ETTh1","--num_experts","3", "--prob_expert"]
        },
        {
            "name": "moe-unc-gating",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "args": ["--task_name","long_term_forecast","--is_training","1","--model_id","test",
             "--model", "PatchTST", "--data", "ETTh1","--num_experts","3", "--prob_expert", "--unc_gating"]
        }
    ]
}